<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
<bean id="mathcheatobj" class="ai.jobiak.object.MathCheat"></bean>
<bean id="sciencecheatobj" class="ai.jobiak.object.ScienceCheat"></bean>
<bean id="javacheatobj" class="ai.jobiak.object.JavaCheat"></bean>
<bean id="stu" class="ai.jobiak.object.Student">
<property name="cheat" ref="javacheatobj"/>
</bean>
</beans>
