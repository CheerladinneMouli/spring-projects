<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context.xsd">
   <context:annotation-config></context:annotation-config>
   <bean id="mathcheatobj" class="ai.jobiak.qualifier.Cheat">
   <property name="nameOfExam" value="Math"/>
   </bean>
   
   <bean id="sciencecheatobj" class="ai.jobiak.qualifier.Cheat">
   <property name="nameOfExam" value="Science"/>
   </bean>
   
   <bean id="javacheatobj" class="ai.jobiak.qualifier.Cheat">
   <property name="nameOfExam" value="Java"/>
   </bean>
   <bean id="student" class="ai.jobiak.qualifier.Student">
   </bean>
   </beans>